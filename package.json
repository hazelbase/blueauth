{
  "name": "blueauth",
  "version": "0.7.10",
  "description": "simple and secure authentication",
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "dev": "nodemon --watch src --ext ts,graphql --exec 'npm run build'",
    "build": "tsc -p tsconfig.json && tsc -p tsconfig.module.json",
    "prepublishOnly": "npm run build",
    "preversion": "npm run test",
    "version": "git add CHANGELOG.md package.json src/*",
    "postversion": "git push && git push --tags",
    "test": "tsc --noEmit && jest src/*",
    "lint": "eslint src"
  },
  "exports": {
    "./nextjs": {
      "require": "./dist/main/platform/nextjs.js",
      "import": "./dist/module/platform/nextjs.js"
    },
    "./lambda": {
      "require": "./dist/main/platform/lambda.js",
      "import": "./dist/module/platform/lambda.js"
    },
    "./express": {
      "require": "./dist/main/platform/express.js",
      "import": "./dist/module/platform/express.js"
    }
  },
  "typings": "dist/module/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "dist/*",
        "dist/module/platform/*.d.ts"
      ]
    }
  },
  "author": "Adrian Artiles",
  "license": "MIT",
  "keywords": [
    "serverless",
    "nextjs",
    "lambda",
    "auth",
    "authentication",
    "blueauth"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/key-lab/blueauth.git"
  },
  "bugs": {
    "url": "https://github.com/key-lab/blueauth/issues"
  },
  "homepage": "https://blueauth.io",
  "files": [
    "dist/main",
    "dist/module",
    "src",
    "!**/*.test.*",
    "!**/*.json",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@jest-mock/express": "^1.4.2",
    "@types/aws-lambda": "^8.10.79",
    "@types/cookie": "^0.4.0",
    "@types/debug": "^4.1.6",
    "@types/express": "^4.17.13",
    "@types/jest": "^26.0.24",
    "@types/jsonwebtoken": "^8.5.3",
    "@types/node": "^15.14.0",
    "@types/nodemailer": "^6.4.2",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.1",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.29.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-plugin-import": "^2.23.4",
    "jest": "^27.0.6",
    "nodemon": "^2.0.12",
    "ts-jest": "^27.0.3",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "cookie": "^0.4.1",
    "debug": "^4.3.2",
    "express-graphql": "^0.12.0",
    "graphql": "^15.5.1",
    "graphql-type-json": "^0.3.2",
    "jsonwebtoken": "^8.5.1",
    "nodemailer": "^6.6.2"
  }
}
